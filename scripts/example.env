# Viam API credentials
API_KEY=your-api-key
API_KEY_ID=your-api-key-id
ORG_ID=your-org-id
LOCATION_ID=your-location-id
WIFI_SSID=your-wifi-network
WIFI_PASSWORD=your-wifi-password
DEVICE='name-or-id-of-your-device'        # use quotes if name has spaces, e.g. 'Julies iPhone'
PLATFORM=ios                              # "ios" or "android"

# Needed for Fastlane (iOS only)
MATCH_PASSWORD=your-match-passphrase
MATCH_KEYCHAIN_PASSWORD=your-keychain-password

# Optional: uncomment to override defaults
# RELEASE=false                           # default: true for iOS, false for Android
# VERBOSE=false                           # default: true
